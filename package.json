{
  "name": "markdown-pro",
  "version": "1.0.16",
  "description": "Markdown Pro",
  "keywords": [
    "markdown",
    "markdown-pro",
    "parser",
    "typescript",
    "ts",
    "flow",
    "flowjs"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/markdown-pro.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webbestmaster/markdown-pro/issues"
  },
  "homepage": "https://github.com/webbestmaster/markdown-pro#readme",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "start-demo": "cross-env NODE_ENV=development BUILD_TYPE=site webpack-cli serve --progress",
    "start-demo:hot": "cross-env NODE_ENV=development BUILD_TYPE=site webpack-cli serve --progress --hot",
    "build-demo": "cross-env NODE_ENV=production BUILD_TYPE=site webpack --progress",
    "build": "cross-env NODE_ENV=production BUILD_TYPE=lib webpack --progress",
    "remove-extra": "rm -rf ./coverage && rm -rf ./.nyc_output",
    "check": "npm run remove-extra; npm run eslint; npm run stylelint; npm run flow;",
    "check:fix": "npm run remove-extra; npm run prettier; npm run eslint:fix; npm run stylelint:fix; npm run flow;",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**/*.scss",
    "stylelint:fix": "stylelint ./**/*.scss --fix",
    "flow": "flow stop; flow;",
    "flow-stop": "flow stop;",
    "prettier": "prettier --write \"./**/*.js\"",
    "mdlint": "eslint --ext md .",
    "mdlint:fix": "eslint --ext md . --fix",
    "flow-coverage": "flow-coverage-report --config ./flow-coverage-config.json",
    "zip": "zip -9 --exclude=*node_modules* --exclude=*.git* -r my-project_`git symbolic-ref --short HEAD`-`date +%Y-%m-%d--%H-%M-%S` ./",
    "test": "cross-env NODE_ENV=test babel-node node_modules/.bin/_mocha ./test/",
    "test-coverage": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text --reporter=lcov babel-node node_modules/.bin/_mocha ./test/",
    "coveralls": "npm run test-coverage && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-strict-mode": "^7.12.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-flow": "^7.13.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.1",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "css-modules-flow-types-loader": "^1.4.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.23.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^5.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.0.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.148.0",
    "flow-coverage-report": "^0.8.0",
    "flow-typed": "^3.3.1",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.1",
    "mocha": "^8.3.2",
    "node-sass": "^5.0.0",
    "nyc": "^15.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^11.0.1",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.30.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
