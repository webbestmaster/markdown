{
  "name": "markdown-pro",
  "version": "0.0.20",
  "description": "Markdown-pro - professional markdown parser.",
  "keywords": [
    "markdown",
    "parser",
    "markdown-pro"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/markdown.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webbestmaster/markdown/issues"
  },
  "homepage": "https://github.com/webbestmaster/markdown#readme",
  "scripts": {
    "start": "webpack-dev-server --progress --no-inline",
    "start:hot": "webpack-dev-server --progress --hot",
    "build": "cross-env NODE_ENV=production webpack --mode=production --progress",
    "check": "npm run eslint; npm run stylelint; npm run flow;",
    "check:fix": "npm run prettier; npm run eslint:fix; npm run stylelint:fix; npm run flow;",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**/*.scss",
    "stylelint:fix": "stylelint ./**/*.scss --fix",
    "flow": "flow stop; flow;",
    "flow-stop": "flow stop;",
    "prettier": "prettier --write \"./**/*.js\"",
    "mdlint": "eslint --ext md .",
    "mdlint:fix": "eslint --ext md . --fix",
    "flow-coverage": "flow-coverage-report --config ./flow-coverage-config.json",
    "zip": "zip -9 --exclude=*node_modules* --exclude=*.git* -r my-project_`git symbolic-ref --short HEAD`-`date +%Y-%m-%d--%H-%M-%S` ./",
    "pre-commit-hook": "npm run eslint && npm run stylelint && npm run flow",
    "test-unit": "babel-node node_modules/.bin/_mocha ./test/",
    "test-unit-coverage": "nyc --reporter=html --reporter=text babel-node node_modules/.bin/_mocha ./test/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {},
  "devDependencies": {
